<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadapta" version="1.0"/>
  <object class="GtkBox" id="widget">
    <property name="spacing">12</property>
    <property name="homogeneous">True</property>
    <property name="width-request">600</property>
    <property name="height-request">200</property>
    <child>
      <object class="AdapNavigationView">
        <style>
          <class name="docs-background"/>
        </style>
        <child>
          <object class="AdapNavigationPage">
            <property name="title">Page 1</property>
            <property name="child">
              <object class="AdapToolbarView">
                <child type="top">
                  <object class="AdapHeaderBar">
                    <property name="show-start-title-buttons">False</property>
                    <property name="show-end-title-buttons">False</property>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdapNavigationPage">
            <property name="title">Page 2</property>
            <property name="child">
              <object class="AdapToolbarView">
                <child type="top">
                  <object class="AdapHeaderBar">
                    <property name="show-start-title-buttons">False</property>
                    <property name="show-end-title-buttons">False</property>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdapNavigationView">
        <style>
          <class name="docs-background"/>
        </style>
        <child>
          <object class="AdapNavigationPage">
            <property name="title">Page 1</property>
            <property name="child">
              <object class="AdapToolbarView">
                <child type="top">
                  <object class="AdapHeaderBar">
                    <property name="show-start-title-buttons">False</property>
                    <property name="show-end-title-buttons">False</property>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdapNavigationPage" id="nav-page">
            <property name="title">Page 2</property>
            <property name="child">
              <object class="AdapToolbarView">
                <child type="top">
                  <object class="AdapHeaderBar">
                    <property name="show-start-title-buttons">False</property>
                    <property name="show-end-title-buttons">False</property>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>

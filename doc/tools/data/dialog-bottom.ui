<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadapta" version="1.0"/>
  <object class="AdapWindow" id="window">
    <property name="default-width">360</property>
    <property name="default-height">400</property>
    <property name="content">
      <object class="AdapToolbarView">
        <child type="top">
          <object class="AdapHeaderBar">
            <property name="show-title">False</property>
          </object>
        </child>
      </object>
    </property>
  </object>
  <object class="AdapDialog" id="widget">
    <property name="content-width">400</property>
    <property name="content-height">300</property>
    <property name="child">
      <object class="AdapToolbarView">
        <child type="top">
          <object class="AdapHeaderBar">
            <property name="show-title">False</property>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>

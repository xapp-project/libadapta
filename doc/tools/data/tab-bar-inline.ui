<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadapta" version="1.0"/>
  <object class="GtkWindow">
    <property name="decorated">False</property>
    <property name="default-width">600</property>
    <property name="child">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdapTabBar" id="widget">
            <property name="view">view</property>
            <style>
              <class name="inline"/>
            </style>
          </object>
        </child>
        <child>
          <object class="AdapTabView" id="view">
            <child>
              <object class="AdapTabPage">
                <property name="title">Page 1</property>
                <property name="child">
                  <object class="AdapBin"/>
                </property>
              </object>
            </child>
            <child>
              <object class="AdapTabPage">
                <property name="title">Page 2</property>
                <property name="child">
                  <object class="AdapBin"/>
                </property>
              </object>
            </child>
            <child>
              <object class="AdapTabPage">
                <property name="title">Page 3</property>
                <property name="child">
                  <object class="AdapBin"/>
                </property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
